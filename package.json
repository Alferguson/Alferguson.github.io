{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "gatsby clean && gatsby build",
    "dev": "cross-env GATSBY_GRAPHQL_IDE=playground gatsby develop",
    "clean": "gatsby clean",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run dev",
    "serve": "gatsby serve",
    "test": "jest",
    "analyze": "cross-env ANALYZE_BUNDLE_SIZE=true gatsby build",
    "deploy": "gatsby clean && gatsby build && gh-pages -d public -b master",
    "deploy-without-build": "gh-pages -d public -b master"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.6.18",
    "@mdx-js/react": "^1.6.18",
    "gatsby": "^2.24.73",
    "gatsby-plugin-mdx": "^1.2.46",
    "gatsby-plugin-react-helmet": "^3.3.14",
    "gatsby-plugin-sharp": "^2.6.40",
    "gatsby-plugin-transition-link": "1.20.2",
    "gatsby-source-filesystem": "^2.3.34",
    "gsap": "^3.5.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.3"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "babel-jest": "^26.5.2",
    "babel-preset-gatsby": "^0.5.13",
    "cross-env": "^7.0.2",
    "eslint": "^7.11.0",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.4",
    "gh-pages": "^3.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.5.2",
    "prettier": "^2.1.2",
    "react-test-renderer": "^16.13.1"
